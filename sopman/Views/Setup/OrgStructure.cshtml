@using Microsoft.AspNetCore.Identity
@{
    ViewData["Title"] = "Organisational Structure";
}

<div class="container csv-upload">
    <div class="row header">
        <div class="col-12">
            <h1>Define your organisation</h1>
            <p>User instuctions here. Accepted file types and file size limits etc.</p>
        </div>
    </div>
    <div class="row">
        <form action="" method="post" enctype="multipart/form-data">
          
          <label for="file">Filename:</label>
          <input type="file" name="file" id="file" />

          <input type="submit" />
        </form>
    </div>
</div>

